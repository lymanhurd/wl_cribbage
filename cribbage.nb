  RowBox[{"acceptDiscards", "[", 
   RowBox[{"gs_List", ",", " ", "p1State_List", ",", " ", "p2State_List"}], 
   "]"}], " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"{", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
