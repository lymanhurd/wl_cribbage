    RowBox[{"p1", ",", " ", "p2", ",", " ", 
     RowBox[{"newHand", "[", "gs", "]"}], ",", " ", "choices"}], "}"}], " ", "/;",
    " ", 
   RowBox[{
    RowBox[{"Length", "[", 
