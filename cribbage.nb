    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", " ", "-", " ", "1"}], ")"}], "/", "13"}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"rank", "[", "n_Integer", "]"}], " ", ":=", " ", 
