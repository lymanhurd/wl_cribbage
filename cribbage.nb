      RowBox[{"suitLetters", "[", 
       RowBox[{"[", 
        RowBox[{"suit", "[", "i", "]"}], "]"}], "]"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"i", ",", " ", "52"}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"cardName", "[", "c_Integer", "]"}], " ", ":=", " ", 
  RowBox[{"cardNames", "[", 
   RowBox[{"[", "c", "]"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
