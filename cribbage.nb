   RowBox[{"{", 
    RowBox[{"p1_", ",", " ", "p2_", ",", " ", "gs_", ",", " ", "choices_"}], 
    "}"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"d1", " ", "=", " ", 
        RowBox[{"p1", "[", 
         RowBox[{"potentialDiscards", "[", 
          RowBox[{"playerState", "[", 
           RowBox[{"1", ",", " ", "gs"}], "]"}], "]"}], "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"d2", " ", "=", " ", 
        RowBox[{"p2", "[", 
         RowBox[{"potentialDiscards", "[", 
          RowBox[{"playerState", "[", 
           RowBox[{"2", ",", " ", "gs"}], "]"}], "]"}], "]"}]}]}], "}"}], ",",
      "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"p1", ",", " ", "p2", ",", " ", 
       RowBox[{"acceptDiscards", "[", 
        RowBox[{"gs", ",", " ", "d1", ",", " ", "d2"}], "]"}], ",", " ", 
       RowBox[{"Join", "[", 
        RowBox[{"choices", ",", 
         RowBox[{"{", 
