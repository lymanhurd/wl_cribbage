   RowBox[{"{", 
    RowBox[{
    "\"\<Spades\>\"", ",", " ", "\"\<Hearts\>\"", ",", " ", 
     "\"\<Diamonds\>\"", ",", " ", "\"\<Clubs\>\""}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"suitLetters", " ", "=", " ", 
