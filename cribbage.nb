   RowBox[{"player_Integer", ",", " ", "gs_List"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"Join", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "player", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Take", "[", 
      RowBox[{"gs", ",", " ", "2"}], "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"gs", "[", 
       RowBox[{"[", 
        RowBox[{"2", " ", "+", " ", "player"}], "]"}], "]"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", 
         RowBox[{"gs", "[", 
          RowBox[{"[", 
           RowBox[{"2", " ", "+", " ", "player"}], "]"}], "]"}], "]"}], " ", "==",
         " ", "4"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"gs", "[", 
          RowBox[{"[", "5", "]"}], "]"}], ",", " ", 
         RowBox[{"gs", "[", 
          RowBox[{"[", 
           RowBox[{"5", " ", "+", " ", "player"}], "]"}], "]"}]}], "}"}], ",",
        "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"0", ",", " ", 
         RowBox[{"{", "}"}]}], "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"Last", "[", "gs", "]"}], "}"}]}], "\[IndentingNewLine]", "]"}],
    " ", "/;", " ", 
   RowBox[{
    RowBox[{"1", " ", "<=", " ", "player", " ", "<=", " ", "2"}], " ", "&&", 
    " ", 
    RowBox[{
     RowBox[{"Length", "[", "gs", "]"}], " ", "==", " ", "8"}]}]}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.9303993498418083`*^9, 3.930399370297285*^9}, {
