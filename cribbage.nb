    RowBox[{
     RowBox[{"(", 
      RowBox[{"n", " ", "-", " ", "1"}], ")"}], "/", "13"}], "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"rank", "[", "n_Integer", "]"}], " ", ":=", " ", 
  RowBox[{"1", " ", "+", " ", 
   RowBox[{"Mod", "[", 
    RowBox[{
     RowBox[{"n", " ", "-", " ", "1"}], ",", " ", "13"}], "]"}]}]}]}], "Input",
