  RowBox[{
   RowBox[{"ResourceFunction", "[", "\"\<PlayingCardGraphic\>\"", "]"}], "[", 
   RowBox[{
    RowBox[{"{", "n", "}"}], ",", " ", 
    RowBox[{"\"\<CardSpreadAngle\>\"", " ", "->", " ", "0"}], ",", " ", 
    RowBox[{"\"\<ImageSize\>\"", " ", "->", " ", "Small"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"displayCards", "[", "h_List", "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"ResourceFunction", "[", "\"\<PlayingCardGraphic\>\"", "]"}], "[", 
