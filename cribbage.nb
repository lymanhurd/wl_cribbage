    "}"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"p1", ",", " ", "p2", ",", " ", 
     RowBox[{"newHand", "[", "gs", "]"}], ",", " ", "choices"}], "}"}], " ", "/;",
    " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"countHand", "[", 
       RowBox[{
        RowBox[{"gs", "[", 
         RowBox[{"[", "8", "]"}], "]"}], ",", " ", 
        RowBox[{"x_", "?", "Positive"}]}], "]"}], " ", "==", " ", "8"}], 
     ")"}], " ", "&&", " ", 
    RowBox[{"(", 
