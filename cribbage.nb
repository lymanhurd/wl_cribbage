      RowBox[{
       RowBox[{"FreeQ", "[", 
        RowBox[{"sequence", ",", " ", "#"}], "]"}], "  ", "&&", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"value", "[", "#", "]"}], " ", "+", " ", 
          RowBox[{"sequenceValue", "[", "sequence", "]"}]}], " ", "<=", " ", 
         "31"}], ")"}]}], ")"}], "&"}]}], "]"}]}]], "Input",
 InitializationCell->
  True,ExpressionUUID->"e5d8d3e9-0b61-f742-ae8e-b9d0c93a71ae"],

